<!-- views/users/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Groups</title>
</head>
<body>
    <h1>List of Groups</h1>
    <ul>
        <% groupsData.forEach(group => { %>
            <li><%= group.name %> - <%= group.email %><a href="/groups/gotoedit/<%= group._id %>/">Edit</a></li>
        <% }); %>
    </ul>
    <br>
    <div>
        
        <button type="submit"><a href="/groupcreate-form">Create New Group</a></button>

    </div>
</body>
</html>
